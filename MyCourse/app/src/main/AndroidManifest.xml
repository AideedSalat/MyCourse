        </activity>
        <activity android:name=".Semester1Activity" />
        <activity android:name=".Semester2Activity" />
        <activity android:name=".Semester3Activity" />
        <activity android:name=".Semester4Activity" />
        <activity android:name=".Semester5Activity" />
        <activity android:name=".Semester6Activity" />
